<section class="c-block-two-columns-list">
    <div class="container-outter">
        <div class="container-wrapper">
            <div class="container">
                <h2>{{ title|e('wp_kses_post') }}</h2>
                <div class="c-block-two-columns-list__row">
                    {% for item in list %}
                        <div class="c-block-two-columns-list__col">
                            <div class="c-block-two-columns-list__col-inner">
                                {{ item.content|e('wp_kses_post') }}
                            </div>
                        </div>
                    {% endfor %}
                </div>
            </div>
        </div>
    </div>
</section>